import pandas as pd 
import numpy as np 
data = { 
    'Age': [25, 30, np.nan, 22, 28, np.nan, 35], 
    'Gender': ['Male', 'Female', 'Male', np.nan, 'Female', 'Female', np.nan], 
    'Salary': [25000, 30000, 500000, 27000, 26000, 28000, 29000]   # note: 500000 is an 
outlier 
} 
df = pd.DataFrame(data) 
 
df['Age'] = df['Age'].fillna(df['Age'].mean()) 
df['Gender'] = df['Gender'].fillna(df['Gender'].mode()[0]) 
Q1, Q3 = df['Salary'].quantile([0.25, 0.75]) 
IQR = Q3 - Q1 
lower = Q1 - 1.5 * IQR 
upper = Q3 + 1.5 * IQR 
df = df[(df['Salary'] >= lower) & (df['Salary'] <= upper)] 
df['Norm_Age'] = (df['Age'] - df['Age'].min()) / (df['Age'].max() - df['Age'].min()) 
df['Std_Salary'] = (df['Salary'] - df['Salary'].mean()) / df['Salary'].std() 
 
print("\nProcessed Data:\n", df)
